{
  "name": "ipfs-web-upload",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:export": "next build && next export",
    "build:ipfs": "yarn run build:export && esm-ts-node ./script/upload-to-ipfs.ts",
    "ci:install": "yarn add --disable-self-update-check --prefer-offline ipfs ipfsd-ctl @bluelovers/fast-glob fast-glob",
    "dev": "next dev",
    "ncu": "npx yarn-tool ncu -u && npx yarn-tool install && yarn run sort",
    "prepublishOnly": "yarn run build:ipfs",
    "sort": "npx yarn-tool sort",
    "start": "next start"
  },
  "dependencies": {
    "@bluelovers/ipfs-http-client": "^1.0.40",
    "@lazy-ipfs/buffer-json": "^1.0.1",
    "@tanker/file-reader": "^2.3.0",
    "array-hyper-unique": "^1.1.0",
    "bluebird": "^3.7.2",
    "buffer": "^5.5.0",
    "consola": "^2.11.3",
    "cross-fetch": "^3.0.4",
    "fetch-ipfs": "^1.0.46",
    "ipfs-browser-util": "^1.0.2",
    "ipfs-http-client": "^43.0.0",
    "ipfs-server-list": "^1.0.27",
    "ipfs-util-lib": "^1.0.26",
    "is-ipfs": "^0.6.3",
    "next": "9.3.4",
    "poke-ipfs": "^1.0.16",
    "pretty-bytes": "^5.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^10.2.2",
    "sass": "^1.26.3",
    "to-ipfs-url": "^1.0.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.5",
    "ts-node": "^8.8.1",
    "typescript": "^3.9.0-dev.20200330",
    "use-ipfs": "^1.0.47"
  }
}
