{
  "name": "ipfs-web-upload",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "next dev -p 9999",
    "build": "next build",
    "build:export": "next build && next export",
    "build:ipfs": "yarn run build:export && esm-ts-node ./script/upload-to-ipfs.ts",
    "prepublishOnly": "yarn run build:ipfs",
    "ci:install": "yarn add --disable-self-update-check --prefer-offline ipfs ipfsd-ctl @bluelovers/fast-glob fast-glob",
    "export": "next export",
    "ncu": "npx yarn-tool ncu -u && npx yarn-tool install && yarn run sort",
    "sort": "npx yarn-tool sort"
  },
  "dependencies": {
    "@bluelovers/ipfs-http-client": "^1.0.48",
    "@lazy-ipfs/buffer-json": "^1.0.2",
    "@lazy-ipfs/detect-ipfs-companion": "^1.0.5",
    "@lazy-ipfs/ipfs-protocol": "^1.0.1",
    "@lazy-ipfs/ipfs-subdomain": "^1.0.4",
    "@lazy-ipfs/to-cid": "^1.0.1",
    "@tanker/file-reader": "^2.3.1",
    "array-hyper-unique": "^1.1.0",
    "bluebird": "^3.7.2",
    "buffer": "^5.6.0",
    "cids": "^0.8.0",
    "consola": "^2.12.1",
    "cross-fetch": "^3.0.4",
    "fetch-ipfs": "^1.0.55",
    "ipfs-browser-util": "^1.0.3",
    "ipfs-http-client": "^44.0.3",
    "ipfs-server-list": "^1.0.32",
    "ipfs-util-lib": "^1.0.33",
    "is-ipfs": "^1.0.3",
    "next": "9.4.1",
    "poke-ipfs": "^1.0.18",
    "pretty-bytes": "^5.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^11.0.1",
    "sass": "^1.26.5",
    "to-ipfs-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.31",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^4.0.0-dev.20200511",
    "use-ipfs": "^1.0.55"
  }
}
